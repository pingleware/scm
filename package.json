  "bugs": {
    "url": "https://pingleware.support"
  },
  "homepage": "https://github.com/pingleware/scm#readme",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  }
}
